@model IEnumerable<OA.WebApp.ViewModels.JournalDto>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<h1>日记账</h1>

@foreach (var item in Model)
{
    @Html.DisplayFor(a => item.ID);
    @Html.DisplayFor(a => item.RecordDate);
    @Html.DisplayFor(a => item.Summary);
}